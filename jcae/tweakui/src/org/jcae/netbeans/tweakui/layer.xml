<filesystem>
    <folder name="Toolbars">
        <file name="Standard.xml" url="Standard.xml"/>
        <folder name="Build_hidden"/>
    </folder>

    <folder name="Menu">
        <file name="Tools_hidden"/>
        <folder name="View">
            <file name="org-netbeans-core-actions-HTMLViewAction.instance_hidden"/>
        </folder>
        <folder name="BuildProject_hidden"/>
        <folder name="RunProject_hidden"/>

        <folder name="File">
            <file name="org-openide-actions-PageSetupAction.instance_hidden"/>
            <file name="org-openide-actions-PrintAction.instance_hidden"/>
        </folder>

        <folder name="GoTo_hidden"/>
        <folder name="Window">
            <file name="CloneDocumentAction.shadow_hidden"/>
            <file name="CloseAllButThisAction.shadow_hidden"/>
            <file name="CloseAllDocumentsAction.shadow_hidden"/>
            <file name="DocumentsAction.shadow_hidden"/>
            <file name="Navigator_hidden"/>
            <file name="Output_hidden"/>
            <file name="ProgressListAction.shadow_hidden"/>
            <file name="Separator4.instance_hidden"/>
            <file name="SwitchToRecentDocumentAction.shadow_hidden"/>
            <file name="ViewRuntimeTabAction.shadow_hidden"/>
            <file name="org-netbeans-modules-options-OptionsWindowAction.instance">
                <attr name="position" intvalue="0"/>
            </file>
            <file name="org-netbeans-core-io-ui-IOWindowAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/org-netbeans-core-io-ui-IOWindowAction.instance"/>
                <attr name="position" intvalue="900"/>
            </file>
            <file name="org-netbeans-modules-autoupdate-ui-actions-PluginManagerAction.instance">
                <attr name="position" intvalue="700"/>
            </file>
        </folder>
    </folder>

    <folder name="Windows2Local">
        <folder name="Modes">
            <folder name="explorer">
                <file name="runtime.wstcref" url="runtimeWstcref.xml"/>
                <file name="favorites.wstcref" url="favoritesWstcref.xml"/>
            </folder>
            <folder name="properties">
                <file name="properties.wstcref" url="propertiesWstcref.xml"/>
            </folder>
            <file name="properties.wsmode" url="propertiesWsmode.xml"/>
        </folder>
        <file name="WindowManager.wswmgr" url="WindowManagerWswmgr.xml"/>
    </folder>
    <folder name="Keymaps">
        <folder name="NetBeans">
            <file name="DELETE.shadow">
                <attr name="originalFile" stringvalue="Actions/Edit/org-openide-actions-DeleteAction.instance"/>
            </file>
            <file name="C-Z.shadow">
                <attr name="originalFile" stringvalue="Actions/Edit/org-openide-actions-UndoAction.instance"/>
            </file>
            <file name="C-Y.shadow">
                <attr name="originalFile" stringvalue="Actions/Edit/org-openide-actions-RedoAction.instance"/>
            </file>
            <file name="C-S.shadow">
                <attr name="originalFile" stringvalue="Actions/System/org-openide-actions-SaveAllAction.instance"/>
            </file>
            <file name="C-X.shadow">
                <attr name="originalFile" stringvalue="Actions/Edit/org-openide-actions-CutAction.instance"/>
            </file>
            <file name="C-C.shadow">
                <attr name="originalFile" stringvalue="Actions/Edit/org-openide-actions-CopyAction.instance"/>
            </file>
            <file name="C-V.shadow">
                <attr name="originalFile" stringvalue="Actions/Edit/org-openide-actions-PasteAction.instance"/>
            </file>
        </folder>
    </folder>
</filesystem>
