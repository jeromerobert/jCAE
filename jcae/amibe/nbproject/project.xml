<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.apisupport.project</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/nb-module-project/3">
            <code-name-base>org.jcae.mesh.amibe</code-name-base>
            <suite-component/>
            <module-dependencies>
                <dependency>
                    <code-name-base>gnu.trove</code-name-base>
                    <build-prerequisite/>
                    <compile-dependency/>
                    <run-dependency>
                        <specification-version>1.0</specification-version>
                    </run-dependency>
                </dependency>
                <dependency>
                    <code-name-base>org.jcae.vtk</code-name-base>
                    <build-prerequisite/>
                    <compile-dependency/>
                    <run-dependency>
                        <specification-version>1.0</specification-version>
                    </run-dependency>
                </dependency>
                <dependency>
                    <code-name-base>vtk</code-name-base>
                    <build-prerequisite/>
                    <compile-dependency/>
                    <run-dependency>
                        <specification-version>1.0</specification-version>
                    </run-dependency>
                </dependency>
            </module-dependencies>
            <public-packages>
                <package>org.jcae.mesh</package>
                <package>org.jcae.mesh.amibe.algos3d</package>
                <package>org.jcae.mesh.amibe.ds</package>
                <package>org.jcae.mesh.amibe.metrics</package>
                <package>org.jcae.mesh.amibe.projection</package>
                <package>org.jcae.mesh.amibe.traits</package>
                <package>org.jcae.mesh.amibe.util</package>
                <package>org.jcae.mesh.bora.ds</package>
                <package>org.jcae.mesh.bora.xmldata</package>
                <package>org.jcae.mesh.cad</package>
                <package>org.jcae.mesh.oemm</package>
                <package>org.jcae.mesh.xmldata</package>
                <package>org.jcae.vtk</package>
            </public-packages>
            <class-path-extension>
                <runtime-relative-path>ext/amibe.jar</runtime-relative-path>
                <binary-origin>../../amibe/dist/amibe.jar</binary-origin>
            </class-path-extension>
            <class-path-extension>
                <runtime-relative-path>ext/jcae-vtk-amibe.jar</runtime-relative-path>
                <binary-origin>../vtk-amibe/dist/jcae-vtk-amibe.jar</binary-origin>
            </class-path-extension>
        </data>
    </configuration>
</project>
